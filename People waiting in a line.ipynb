   "source": [
    "import numpy as np\n",
    "\n",
    "array1=np.arange(1,1001,1) # 1000 people waiting in a line\n",
    "while len(array1)>1:\n",
    "    del_index=np.arange(1,len(array1),2) # Remove people in even places recursively\n",
    "    new=np.delete(array1,del_index)\n",
    "    array1=new\n",
    "    print(array1)"
   ]
